<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5p]%d{HH\:mm\:ss,SSS}, [Class]%-c{1}, %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>

        <!--common property-->
        <!--<logger name="com.netease" level="error" additivity="false">-->
            <!--<appender-ref ref="DailyRollingFile"/>-->
        <!--</logger>-->
    </Loggers>
</Configuration>